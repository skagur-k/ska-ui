{
	"packages": ["./packages/plugin", "./packages/core/*"],
	"version": "independent",
	"npmClient": "npm",
	"useWorkspaces": true,
	"registry": "https://npm.pkg.github.com/",
	"command": {
		"version": {
			"exact": true
		},
		"publish": {
			"conventionalCommits": true,
			"message": "chore(release): publish",
			"allowBranch": "main"
		}
	},
	"$schema": "node_modules/lerna/schemas/lerna-schema.json",
	"useNx": false
}
